{
    "name": "OmniHistoryPlus",
    "description": "Search your entire browser history from the Omnibar. Hit 'f' followed by a tab or space to activate this feature, then just search!",
    "version": "1.0.2",
    "manifest_version": 3,
    "minimum_chrome_version": "120",
    "icons": {
        "16": "/img/logo16.png",
        "19": "/img/logo19.png",
        "48": "/img/logo48.png",
        "128": "/img/logo128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/img/logo16.png",
            "19": "/img/logo19.png",
            "48": "/img/logo48.png",
            "128": "/img/logo128.png"
        }
    },
    "options_page": "options.html",
    "permissions": [
        "activeTab",
        "history",
        "storage"
    ],
    "omnibox": {
        "keyword": "f"
    }
}
